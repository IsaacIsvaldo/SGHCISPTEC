												<div class="card-actions float-end">
													<div class="dropdown position-relative">
														<a href="#" data-bs-toggle="dropdown" data-bs-display="static">
															<i class="align-middle" data-feather="more-horizontal"></i>
														</a>

														<div class="dropdown-menu dropdown-menu-end">
															<a class="dropdown-item" href="#">Acção</a>

														</div>
													</div>
												</div>
												<h5 class="card-title mb-0">Meus Relatorios</h5>
											</div>
											<table id="datatables-dashboard-traffic" class="table table-striped my-0">
												<thead>
													<tr>
														<th>#</th>
														<th class="text-end">Internações</th>
														<th class="d-none d-xl-table-cell text-end">Criação</th>
														<th class="d-none d-xl-table-cell text-end">Atualização</th>
														<th class="d-none d-xl-table-cell text-end">Horas</th>
													</tr>
												</thead>
												<tbody>
													
													<% meusralatorios.reverse().map(m=>{ %>
													<tr>
														<td><%= m.idRelatorioE %></td>
														<td class="text-end"><%= m.internacoes %></td>
														<td class="d-none d-xl-table-cell text-end"><%= m.createdAt %></td>
														<td class="d-none d-xl-table-cell text-end text-success"><%= m.updatedAt %>
														</td>
														<td class="d-none d-xl-table-cell text-end"><%= m.hora %></td>
													</tr>
													<%}) %>
												</tbody>
											</table>
										</div>
									</div>
								</div>

							</div>
						</main>

                        <%- include('./Partes/footer') %> 
				</div>
		</div>


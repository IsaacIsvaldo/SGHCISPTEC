														<div class="card-body">
															<div class="row">
																<div class="col mt-0">
																	<h5 class="card-title">Historicos </h5>
																</div>

																<div class="col-auto">
																	<div class="stat stat-sm">
																		<i class="align-middle"
																			data-feather="arrow-up-right"></i>
																	</div>
																</div>
															</div>
															<span class="h1 d-inline-block mt-1 mb-4"><%= historico.length%> </span>
															<div class="mb-0">
																<span class="badge badge-soft-success me-2"> +3.65%
																</span>
																<span class="text-muted">Ultima semana</span>
															</div>
														</div>
													</div>
												</div>
											</div>
											<div class="row d-lg-none d-xxl-flex">
												<div class="col-sm-6 col-lg-12 col-xxl-6 d-flex">
													<div class="card flex-fill">
														<div class="card-body">
															<div class="row">
																<div class="col mt-0">
																	<h5 class="card-title">Secção</h5>
																</div>

																<div class="col-auto">
																	<div class="stat stat-sm">
																		<i class="align-middle"
																			data-feather="clock"></i>
																	</div>
																</div>
															</div>
															<span class="h1 d-inline-block mt-1 mb-4">
																0

															</span>
															<div class="mb-0">
																<span class="badge badge-soft-success me-2"> +2.25%
																</span>
																<span class="text-muted">Ultima semana</span>
															</div>
														</div>
													</div>
												</div>
												<div class="col-sm-6 col-lg-12 col-xxl-6 d-flex">
